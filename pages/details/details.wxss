page{
  background-color:#f4f4f4;
  min-height: 100%;
}
.detail-tab{
  background: #FFF;
  display:flex; 
  flex-direction:row;
  width: 100%;
  height: 39px;
  justify-items: center;
  align-items: center;
  justify-content: center;
  text-justify: center;
}
.tab-item{
  color: #999999;
  width: 50%;
  font-size: 33rpx;
}

.l{
  text-align: right;
  margin-right: 6px;
}

.r{
  margin-left: 6px;
}

.selected{
  color: #333333;
}

.container{
    margin-bottom: 100rpx;
}
.goodsimgs{
  width: 750rpx;
  height: 549rpx;
}

.goodsimgs image{
  width: 750rpx;
  height: 750rpx;
}

.detail-info{
   background: #FFF;
   display: flex;
   justify-content: space-between;
}

.detail-info .info{
  width: 76%;
  margin-left: 13px;
  padding-top: 13px;
}
.info .name {
  display: block;
  color: #5a5a5a;
  line-height: 50rpx;
  font-size: 33rpx;
  font-weight: bold;
}

.info .desc {
  display: block;
  color: #a0a0a0;
  line-height: 39rpx;
  font-size: 30rpx;
  margin-top: 5px;
}

.share{
  margin-right: 13px;
  margin-top: 29px;
  justify-content: center;
  align-content: center;
  align-items: center;
}

.share-icon{
  width: 36rpx;
  height: 36rpx;
}

.share-text{
  color: #72cfdd;
  font-size: 25rpx;

}

.status-info{
  background: #FFF;
}

.status-button{
  display: flex;
  height: 56px;
  margin: 13px 13px;
  background: #fcf4e1;
  color: #666;
  border-radius: 6rpx;
  justify-content: space-between;
  align-content: center;
  align-items: center;
}

.join-tip{
  color: #888;
  margin-right:9px;
}
.line{
  margin-left: 13px;
  margin-right: 13px;
  background: #FFF;
  border-top: 0.1rpx solid #d1d2d3;
}

.desc-info{
   background: #FFF;
  
}

.title{
  margin: 13px 13px;
  display: block;
  color: #7a7a7a;
  line-height: 39rpx;
  font-size: 33rpx;
  font-weight: bold;
}

.desc-info .content{
  margin-left: 13px;
  margin-right: 13px;
  color: #666666;
  line-height: 49rpx;
  font-size: 30rpx;
}


.merchant-info{
   background: #FFF;
}

.merchant-box{
  height: 103px;
  margin-left: 13px;
  margin-right: 13px;
  background: #FFF;
  border:1rpx solid #dfe8f1;
  border-radius: 6rpx;
  -moz-box-shadow:3px 6px 9px #dfe8f1; 
  -webkit-box-shadow:3px 6px 9px #dfe8f1; 
  box-shadow:3px 6px 9px#dfe8f1;
  padding-left:103px;
  padding-top:11px;
}

.merchant-label{
  padding:3px 6px;
  background:#72cfdd;
  color:#fff;
  border-radius:6rpx;

}

.merchant-name{
  color: #333;
  font-size: 33rpx;
  line-height:26px;
}
.merchant-c{
  color: #888888;
  font-size: 30rpx;
  height:25px;
  line-height:30px;

}
.merchant-avatar{
  width: 149rpx;
  height: 149rpx;
  border-radius: 50%;
  position: relative;
  left:30px;
  top:39px;
}

.price-content{
  display: flex;
  justify-content: space-between;
  align-items: center;
}


.bottom-btn{
    position: fixed;
    left:0;
    bottom:0;
    z-index: 10;
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: space-around;
    background: #fff;
}

.bottom-btn .p{
    border-top: 1px solid #f4f4f4;
    height: 100rpx;
    width: 80%;
    display: flex;
    align-items: center;
}

.buy-price{
  color: #f0c33d;
  font-size: 35rpx;
  margin-right:3px;
}
.market-price{
  margin-right: 13px;
  color: #aeaeae;
  font-size: 24rpx;
  height: 24px;
  line-height: 24px;
  text-decoration: line-through;
}
.check-icon{
  width: 25rpx;
  height: 25rpx;
  margin-right: 3px;
}
.ticket{
  margin-left: 6px;
  font-size: 24rpx;
  align-content: center;
  align-items: center;
}
.ticket-title{
   font-size: 24rpx;
   color: #787878;
}
.bottom-btn .c{
    float: left;
    height: 100rpx;
    line-height: 96rpx;
    flex: 1;
    text-align: center;
    color: #333;
    border-top: 1px solid #f4f4f4;
    border-bottom: 1px solid #f4f4f4;
}

.bottom-btn .r{
    width: 103px;
    border:1px solid #72cfdd;
    background: #72cfdd;
    float: left;
    height: 100rpx;
    line-height: 96rpx;
    text-align: center;
    color: #fff;
}
@import "../../lib/wxParse/wxParse.wxss";

.attr-pop{
    width: 100%;
    height: 100%;
    padding: 31.25rpx;
    background: #fff;
}

.attr-pop .img-info{
    width: 687.5rpx;
    height: 177rpx;
    overflow: hidden;
    margin-bottom: 41.5rpx;
}

.attr-pop .img{
    float: left;
    height: 177rpx;
    width: 177rpx;
    background: #f4f4f4;
    margin-right: 31.25rpx;
}

.attr-pop .info{
    float: left;
    height: 177rpx;
    display: flex;
    align-items: center;
}

.attr-pop .p{
    font-size: 33rpx;
    color: #333;
    height: 33rpx;
    line-height: 33rpx;
    margin-bottom: 10rpx;
}

.attr-pop .a{
    font-size: 29rpx;
    color: #333;
    height: 40rpx;
    line-height: 40rpx;
}

.spec-con{
    width: 100%;
    height: auto;
    overflow: hidden;
}

.spec-con .name{
    height: 32rpx;
    margin-bottom: 22rpx;
    font-size: 29rpx;
    color: #333;
}

.spec-con .values{
    height: auto;
    margin-bottom: 31.25rpx;
    font-size: 0;
}

.spec-con .value{
    display: inline-block;
    height: 62rpx;
    padding: 0 35rpx;
    line-height: 56rpx;
    text-align: center;
    margin-right: 25rpx;
    margin-bottom: 16.5rpx;
    border: 1px solid #333;
    font-size: 25rpx;
    color: #333;
}

.spec-con .value.disable{
    border: 1px solid #ccc;
    color: #ccc;
}

.spec-con .value.selected{
    border: 1px solid #b4282d;
    color: #b4282d;
}

.number-item .selnum{
    width: 169rpx;
    height: 71rpx;
    display: flex;
    margin-right:11px;

}

.number-item .cut{
    width: 50rpx;
    height: 50rpx;
    text-align: center;
    line-height: 45rpx;
    border: 1px solid #72cfdd;
    border-radius: 50%;
    color: #72cfdd;
}

.number-item .number{
    flex: 1;
    height: 50rpx;
    text-align: center;
    line-height: 50rpx;
    float: left;
}

.number-item .add{
   width: 50rpx;
    height: 50rpx;
    text-align: center;
    line-height: 45rpx;
    border: 1px solid #72cfdd;
    border-radius: 50%;
    color: #FFF;
    background: #72cfdd;
}


