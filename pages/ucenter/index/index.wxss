.mylists{
  margin-top: 10rpx;
  background: #fff;
}
.mylist-cells{
  padding-left: 10rpx;
  display: flex;
  flex-direction: row;
}
.mylist-cells text{
  display: block;
}
.mylist-cells .mylist-cell-desc{
  flex: 1;
  border-bottom: 1px solid #f4f4f4;
  padding: 30rpx 0;
  height: 45rpx;
  line-height: 45rpx;
  box-sizing: content-box;
  font-size: 32rpx;
}
.mylist-cells .mylist-cell-image{
  width: 35rpx;
  height: 35rpx;
  padding: 35rpx;
  box-sizing: content-box;
}
.mylist-cells .mylist-cell-image image{
  width: 35rpx;
  height: 35rpx;
}

.noborder{
  border: 0!important;
}