    background-size: 52.803rpx;
}

.user-menu .icon.wallet{
    background: url(https://www.resource.yuntuodev.cn/resource/images/weixin/mall/wallet.png)  no-repeat;
    background-size: 60.803rpx;
}

.user-menu .icon.gift{
