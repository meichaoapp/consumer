.user-menu .txt{
    display: block;
    height: 24rpx;
    width: 100%;
    font-size: 24rpx;
    color:#333;
}

.logout{
    margin-top: 50rpx;
    height: 101rpx;
    width: 100%;
    line-height: 101rpx;
    text-align: center;
    background: #fff;
    color: #333;
    font-size: 30rpx;
}

.my-buy{
  margin-top: 13px;
}
.about-link {
  border-top: none;
}
.no-top-border{
  border-top: none;
}
.no-bottom-border{
  border-bottom: none;
}
.more-btn {
  display: flex;
  background-color: #fff;
  min-height: 85rpx;
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
  align-items: center;
  font-size: 13pt;
  padding: 0 30rpx;
}

.more-btn-t {
  display: flex;
  background-color: #fff;
  min-height: 85rpx;
  border-bottom: 1px solid #e5e5e5;
  align-items: center;
  font-size: 13pt;
  padding: 0 30rpx;
}

.btn-icon {
  width: 36rpx;
  height: 36rpx;
  margin-right: 23rpx;
}
.btn-text {
  font-size: 10pt;
  flex: 1;
}
.btn-open {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  margin-left: 10rpx;
}
.btn-enter {
  width: 18rpx;
  height: 27rpx;
}
