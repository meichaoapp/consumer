page{
    height: 100%;
    width: 100%;
    background: #f5f5f5;
  
}

.container{
  height: 100%;
  width: 100%;
  overflow: hidden;
}

.userinfobox1{
margin-top: 20rpx;
background: #fff;
 padding:0 30rpx;
 /* border-bottom: 1rpx solid #E4E4E4;
 margin-bottom: 25rpx; */
}
.userinfobox2{
background: #fff;
 padding:0 30rpx;
 border-bottom: 1rpx solid #E4E4E4;
 margin-bottom: 37rpx;
}
.cell_input{
  padding: 10rpx 0;
 border-bottom: 1rpx solid #E4E4E4;
}
.u-label{
width: 160rpx;
height:80rpx;
min-height:80rpx;
line-height:80rpx;
font-size: 30rpx!important;
color: #333;
text-overflow:clip;
overflow:hidden;
white-space:nowrap;
font-family:UICTFontTextStyleBody;

}
.u-bd{
flex: 1;
}
.u-bd .weui-inputs{
  border: 0;
  margin: 0;
  padding: 0;
  background: transparent;
  color: #333;
  font-size:28rpx;
  height:80rpx;
min-height:80rpx;
line-height:80rpx;

}
.u-validate,.selbtn{
  position: relative;

}
.u-validate input{
width: 150rpx!important;
}
.nplaceholder{
    color: #999;
    font-size:28rpx;
}
.noborder{
  border: 0!important;
}
.getcodebtn{
    width:194rpx;
    height:52rpx;
    color:#FF9000;
    border:1px solid #FF9000;
    text-align:center;
    line-height:52rpx;
}

.selbtn picker{
  font-size:28rpx;
  height:80rpx;
min-height:80rpx;
line-height:80rpx;
text-overflow:clip;
color: #333;
overflow:hidden;
white-space:nowrap;
font-family:UICTFontTextStyleBody;
}
.guid{
  position: absolute;
  right: 0;
  top:25rpx;
  width: 30rpx;
}
.guid image{
  width: 15rpx;
  height: 26rpx;
}
.submitUserInfobtn-bd{
 display: flex;
 justify-content: center;
}
.submitUserInfobtn-bd view{
    width:440rpx;
    height:90rpx;
    background: #89C765;
    box-shadow: 0 4px 16px 0 #89C765;
    border-radius: 35px;
    font-size:30rpx;
    color:#fff;
    line-height:90rpx;
    text-align:center;
}
